<div class="content">
  <h1>Fotorama</h1>

  <p class="lead">A simple, stunning, powerful jQuery gallery. Licensed <a href="/license/">under the <abbr>MIT</abbr></a>.</p>

  <% renderMenu = (items) => %>
    <% for item in items: %>
      <% if item.order: %>
        <% if item.url.match(/set-up/): %>
          <h2><%= item.title %></h2>
        <%- @partial('documents' + item.url + 'index.html.md') %>
        <% else: %>
          <h2 class="front-menu"><a href="<%= item.url %>"><%= item.title %></a></h2>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
  <%= renderMenu @generateMenu(@document.url) %>
</div>